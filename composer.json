{
    "name": "yaranliu/gadget",
    "description": "A utility library for Laravel Applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ufuk YARANLI",
            "email": "ufuk@yaranli.net",
            "homepage": "https://github.com/yaranliu"
        }
    ],
    "homepage": "https://github.com/yaranliu/gadget",
    "keywords": ["Laravel", "Gadget", "Utility"],
    "minimum-stability": "dev",
    "require": {
        "ext-mbstring": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5@dev",
        "orchestra/testbench": "^3.8@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yaranliu\\Gadget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yaranliu\\Gadget\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yaranliu\\Gadget\\GadgetServiceProvider"
            ],
            "aliases": {
                "Gadget": "Yaranliu\\Gadget\\Facades\\Gadget"
            }
        }
    }
}
